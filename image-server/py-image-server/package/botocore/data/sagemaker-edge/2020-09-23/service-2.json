      "required": ["AgentVersion", "DeviceName", "DeviceFleetName"],
      "type": "structure"
    },
    "Timestamp": { "type": "timestamp" },
    "Value": { "type": "double" },
    "Version": {
      "max": 64,
      "min": 1,
      "pattern": "[a-zA-Z0-9\\ \\_\\.]+",
      "type": "string"
    }

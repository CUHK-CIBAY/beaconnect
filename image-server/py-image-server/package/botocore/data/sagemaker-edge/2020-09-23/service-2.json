          "documentation": "<p>Returns the version of the agent.</p>",
          "shape": "Version"
        },
        "DeviceFleetName": {
          "documentation": "<p>The name of the fleet that the device belongs to.</p>",
          "shape": "DeviceFleetName"
        },
        "DeviceName": {
          "documentation": "<p>The unique name of the device.</p>",
          "shape": "DeviceName"
        },
        "Models": {

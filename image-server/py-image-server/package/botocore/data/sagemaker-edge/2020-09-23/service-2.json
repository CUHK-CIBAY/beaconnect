      "members": {
        "Message": { "shape": "ErrorMessage" }
      },
      "type": "structure"
    },
    "Metric": {
      "max": 100,
      "min": 4,
      "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*$",
      "type": "string"
    },
    "Model": {
      "documentation": "<p>Information about a model deployed on an edge device that is registered with SageMaker Edge Manager.</p>",

          "documentation": "<p>Information required for model metrics.</p>",
          "shape": "EdgeMetrics"
        },
        "ModelName": {
          "documentation": "<p>The name of the model.</p>",
          "shape": "ModelName"
        },
        "ModelVersion": {
          "documentation": "<p>The version of the model.</p>",
          "shape": "Version"
        }
      },
      "type": "structure"
    },
    "ModelName": {
      "max": 255,
      "min": 4,
      "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*$",
      "type": "string"
    },
    "Models": {
      "member": { "shape": "Model" },
      "type": "list"
